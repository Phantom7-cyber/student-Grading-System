#include <stdio.h>

int main() {
    int marks;
    printf("Enter the marks of student (with %% sign): ");
    scanf("%d%%", &marks); // %d%% reads number followed by % sign

    if (marks > 90 && marks <= 100) {
        printf("Wrong coordinates\n");
    }
    else if (marks >= 80) {
        printf("Grade A\n");
    }
    else if (marks >= 70) {
        printf("Grade B\n");
    }
    else if (marks >= 60) {
        printf("Grade C\n");
    }
    else if (marks >= 40) {
        printf("Grade D\n");
    }
    else {
        printf("Fail\n");
    }
    return 0;
}
